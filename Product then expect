 vector<long long int> productExceptSelf(vector<long long int>& nums) {

        // code here
        vector<long long int> res;
        long long int prod;
        int n=nums.size();
        int i=0;
        for(int j=1;j<n-1;j++){
            prod*=nums[i];
        }
        res.push_back(prod);
        i++;
    return res;
    }
